<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">

    <servlet>
        <servlet-name>checking</servlet-name>
        <servlet-class>controller.ControllerServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>checking</servlet-name>
        <url-pattern>/check</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>servletExe2</servlet-name>
        <servlet-class>controller.AreaCheckServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>servletExe2</servlet-name>
        <url-pattern>/WEB-INF/AreaCheckApp</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>servletExe3</servlet-name>
        <servlet-class>controller.ClearServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>servletExe3</servlet-name>
        <url-pattern>/WEB-INF/ClearApp</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Faces Servlet</servlet-name>
        <servlet-class>javax.faces.webapp.FacesServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>Faces Servlet</servlet-name>
        <url-pattern>*.xhtml</url-pattern>
    </servlet-mapping>
</web-app>

